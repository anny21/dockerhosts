{
    "hosts-folder": "/var/run/docker-hosts",
    "no-docker-wait": 60,
    "between-updates-wait": 2,
    "dnsmasq-executable": "/usr/sbin/dnsmasq",
    "dnsmasq-parameters": [
        "--no-daemon",
        "--clear-on-reload",
        "--no-resolv",
        "--no-hosts",
        "--listen-address=127.0.0.54",
        "--port=53",
        "--hostsdir="
    ]
}
